{
  "manifest_version": 2,

  "name": "dibs filters extension",
  "description": "This extension helps with filtering github pull requests and jira fix versions",
  "version": "1.0.0",

  "content_scripts": [{
    "matches": [
        "https://github.com/*"
      ],
      "js": [
        "sizzle.min.js",
        "ext.js"
      ]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "options": "options.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://github.com/*"
  ]
}
